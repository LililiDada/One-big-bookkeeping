.bill-img{
  width:40rpx;
  height:40rpx;
  margin:10rpx 10rpx 0 0; 
}
.bill-box{
  margin:10rpx 0; 
  /* padding:5rpx 20rpx; */
  color: #878787; 
  background: white
}
.imgstyle{
  width: 80rpx;
  height: 80rpx;
  background: #9bc08a;
  border-radius: 50%;
  display:flex;

}
.billtype{
  width: 50rpx;
  height: 50rpx;
  margin: auto
}
.bill-account{
  float:right;
}
.bill-header,.bill-specific{
  padding:5rpx 20rpx;
}
.bill-header{
  height: 56rpx;
}
.date{
  position: relative;
  top: -3rpx
}
.bill-specific{
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100rpx;
  /* margin-top: 10rpx; */
  /* margin-bottom:10rpx; */
  border-top: 1px solid #f0f0f0;
}
.bill-remark{
  display: flex;
  flex-direction: column;
  width: 80%;
  height: 100%;
  justify-content: center;
  margin-left: 20rpx;
}
.bill-detail{
  line-height: 100rpx;
  color: #e28e9d;
  font-weight: 600;
}
.bill-details{
  line-height: 100rpx;
  color: #5997bb;
  font-weight: 600;
}
.bill-divide{
  width: 100%;
  height:8rpx;
  background-color:#eee;
}
